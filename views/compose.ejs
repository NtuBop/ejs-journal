<%- include("partials/header"); -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle">
  </div>

  <div class="form-group">
    <label>Post</label>
    <textarea name="postBody" class="form-control" rows="7"></textarea>
  </div>
  <button type="submit" class="btn btn-primary mb-2" name="button">Publish</button>
</form>



<%- include("partials/footer"); -%>
